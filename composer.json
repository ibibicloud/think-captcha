{
    "name": "ibibicloud/think-captcha",
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "ibibicloud/think-framework": "8.*"
    },
    "autoload": {
        "psr-4": {
            "think\\captcha\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "think\\captcha\\CaptchaService"
            ],
            "config":{
                "captcha": "src/config.php"
            }            
        }
    }
}
